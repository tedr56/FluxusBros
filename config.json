{
    "audio": ["system:capture_1", 512, 44100],
    "gain": 2,
    "smoothing-bias": 0.2,
    "interfaceControl": true,
    "interfaceCallBack": true,
    "loadPlayers": {
        "Korg": true,
        "Bitstream": true,
        "FingerPlay": false,
        "TestKb": true
    },
    "players": {
        "Korg": {
            "MidiInputPort": {
                "microKONTROL:1": true,
                "microKONTROL:2": true
            },
            "defaultDirectory": "config/",
            "loadFocus": {
                "Korg-Focus": true
            },
            "loadTriggers": {
                "Korg-Triggers": true,
                "Korg-Keyboard": false
            },
            "loadMapping": {
                "Korg-Mapping": true
            },
            "loadVisuals": {
                "Korg-Visuals": true
            }
        },
        "Bitstream": {
            "MidiInputPort": "Bitstream 3X:0",
            "defaultDirectory": "config/",
            "loadTriggers": {
                "Bitstream-Triggers": true
            }
        },
        "FingerPlay": {
            "defaultDirectory": "config/",
            "loadTriggers": {
                "FingerPlay-Triggers": true
            }
        },
        "TestKb":{
            "defaultDirectory": "config/",
            "loadTriggers": {
                "TestKb": true
            }
        }
    }
}


